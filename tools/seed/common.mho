@ = require(['mho:std']);

exports.routeOptions = function(route) {
  route = route .. @route.SetHeaders({
    "Cache-control": "no-cache",
    "Expires": "-1",
  });
  if (@env.get('production') === false) {
    // show stacktraces
    route = route .. @route.DeveloperMode();
  }
  return route;
};

