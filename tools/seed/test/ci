#!/usr/bin/env conductance
// vim: ft=sjs
@ = require('sjs:std');
process.env['BROWSERS'] = 'Firefox'
process.env['USER_BACKEND'] = 'leveldown'
var here = @url.normalize('./', module.id) .. @url.toPath();
@childProcess.run(@path.join(here, '../../gup'), [@path.join(here, '../install')], {'stdio':'inherit'});
@childProcess.run(@path.join(here, 'all'), [], {'stdio':'inherit'});
